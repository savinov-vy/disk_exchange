<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">

    <view entity="exchange_Disk" name="disk-view-edit" extends="_minimal">
        <property name="description"/>
        <property name="genres" view="_minimal"/>
    </view>
    <view entity="exchange_Genre" name="genre-view" extends="_local"/>
    <view entity="exchange_AppUser" name="appUser-view-custom" extends="_minimal">
        <property name="firstName"/>
        <property name="lastName"/>
        <property name="middleName"/>
    </view>
    <view entity="exchange_TakenItem" name="takenItem-view-custom" extends="_local">
        <property name="user" view="_minimal"/>
        <property name="disk" view="_minimal">
        <property name="genres" view="_minimal"/>
        </property>
    </view>
</views>